{
  "name": "socket_test",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./server.js",
    "debug": "nodemon --inspect ./server.js",
    "test": "mocha --recursive ./test/*Test.js --require babel-core/register",
    "migrate/init": "npx db-migrate db:create test",
    "migrate/up": "npx db-migrate up",
    "migrate/down": "npx db-migrate down",
    "migrate/reset": "npx db-migrate reset",
    "migrate/crean": "npx db-migrate db:drop test",
    "watch": "npx webpack -w",
    "build": "npx webpack"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.11.3",
    "db-migrate-pg": "^0.4.0",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "morgan": "^1.8.2",
    "pg": "~7.5.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.3.3",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "undux": "^5.0.0-beta.17"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "nodemon": "~1.18.4"
  }
}
